{
  "name": "nebenan-react-hocs",
  "description": "Collection of React High Order Components",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-react-hocs",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-react-hocs/issues",
  "version": "8.2.0",
  "files": [
    "lib/*.js",
    "lib/*/*.js"
  ],
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm run lint && npm run test && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./lib/ || exit 1",
    "compile": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "build": "npm run compile",
    "prepublishOnly": "npm run check && npm run clean && npm run build",
    "lint": "eslint --ext .jsx,.js ./",
    "lint:tofile": "eslint --ext .jsx,.js --no-color -o ./eslint-report.log ./",
    "test": "TZ='GMT' mocha \"./{,!(node_modules)/**}/*.test.{js,jsx}\"",
    "test:tofile": "TZ='GMT' mocha --no-colors -R spec \"./{,!(node_modules)/**}/*.test.{js,jsx}\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-json-strings": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/plugin-transform-strict-mode": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/register": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browserslist-config-nebenan": "^1.0.0",
    "chai": "^4.2.0",
    "date-fns": "^2.16.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.13.0",
    "eslint-config-nebenan": "^3.0.0",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.15",
    "mocha": "^8.2.0",
    "nebenan-helpers": "^6.0.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "redux": "^4.0.5",
    "sinon": "^10.0.0"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.12.5",
    "lodash": "^4.17.15",
    "nebenan-helpers": "^4.3.0 || ^5.0.0 || ^6.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0 || ^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "redux": "^4.0.5"
  }
}
